  "main": "src/index.ts",
  "scripts": {
    "build": "rimraf ./.build && tsc",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "start:build": "npm run build && node ./.build/index.js",
    "sls:package": "sls package",
    "sls:start": "sls offline start --stage local --host 0.0.0.0",
    "sls:deploy:staging": "sls deploy --stage staging",
    "sls:remove:staging": "sls remove --stage staging",
    "sls:deploy:prod": "sls deploy --stage prod",
    "sls:logs:staging": "sls logs -f api -s staging -t",
    "sls:logs:help": "sls logs --help",
    "format": "prettier --write './{,**/}*.[jt]s'",
    "test": "jest --watch",
    "coverage": "jest --coverage"
  },
  "author": "Jared Orme",
  "license": "ISC",
